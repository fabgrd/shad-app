    "components": {
        "schemas": {
            "BadRequestSchema": {
                "type": "object",
                "properties": {
                    "error": {
                        "type": "string",
                        "description": "Error message",
                        "example": "PATH: [author] ;; MESSAGE: \"author\" is required"
                    }
                },
                "required": [
                    "error"
                ]
            },
            "ServerErrorSchema": {
                "type": "object",
                "properties": {
                    "error": {
                        "type": "string",
                        "description": "Error message",
                        "example": "Internal server error"
                    }
                },
                "required": [
                    "error"
                ]
            },
            "NotFoundErrorSchema": {
                "type": "object",
                "properties": {
                    "error": {
                        "type": "string",
                        "description": "Error message",
                        "example": "Item not found"
                    }
                },
                "required": [
                    "error"
                ]
            },
            "DraftUserSchema": {
                "type": "object",
                "properties": {
                    "username": {
                        "type": "string",
                        "description": "Username",
                        "example": "johndoe01"
                    },
                    "name": {
                        "type": "string",
                        "description": "Name",
                        "example": "John"
                    },
                    "password": {
                        "type": "string",
                        "description": "Password",
                        "example": "password"
                    },
                    "email": {
                        "type": "string",
                        "description": "Email",
                        "example": "johndoe@john.com"
                    },
                    "genre": {
                        "type": "string",
                        "description": "Genre",
                        "example": "M"
                    },
                    "birthDate": {
                        "type": "string",
                        "description": "Birth date",
                        "example": "1993-03-03"
                    }
                }
            },
            "UserSchema": {
                "type": "object",
                "properties": {
                    "message": {
                        "type": "string",
                        "description": "Message",
                        "example": "Success"
                    },
                    "user": {
                        "type": "object",
                        "properties": {
                            "id": {
                                "type": "string",
                                "description": "User ID",
                                "example": "5e9f9b9b9b9b9b9b9b9b9b9b"
                            },
                            "username": {
                                "type": "string",
                                "description": "Username",
                                "example": "danyDan93"
                            },
                            "name": {
                                "type": "string",
                                "description": "Name",
                                "example": "Daniel"
                            },
                            "email": {
                                "type": "string",
                                "description": "Email",
                                "example": "daniel.tirrier@epitech.eu"
                            },
                            "genre": {
                                "type": "string",
                                "description": "Genre",
                                "example": "M"
                            },
                            "birthDate": {
                                "type": "string",
                                "description": "Birth date",
                                "example": "1993-03-03"
                            },
                            "createdAt": {
                                "type": "string",
                                "description": "Creation date",
                                "example": "2020-04-20T15:00:00.000Z"
                            },
                            "updatedAt": {
                                "type": "string",
                                "description": "Update date",
                                "example": "2020-04-20T15:00:00.000Z"
                            },
                            "refreshToken": {
                                "type": "string",
                                "description": "Refresh token"
                            }
                        }
                    },
                    "token": {
                        "type": "string",
                        "description": "Token"
                    }
                }
            },
            "DraftUserLogin": {
                "type": "object",
                "properties": {
                    "email": {
                        "type": "string",
                        "description": "Email",
                        "example": "daniel.tirrier@epitech.eu"
                    },
                    "password": {
                        "type": "string",
                        "description": "Password",
                        "example": "password"
                    }
                }
            },
            "DraftRefreshToken": {
                "type": "object",
                "properties": {
                    "token": {
                        "type": "string",
                        "description": "Access token"
                    },
                    "refreshToken": {
                        "type": "string",
                        "description": "Refresh token"
                    }
                }
            },
            "DraftRoutineSchema": {
                "type": "object",
                "properties": {
                    "deadline": {
                        "type": "string",
                        "description": "Deadline",
                        "example": "2020-04-20T15:00:00.000Z"
                    },
                    "cheatDay": {
                        "type": "boolean",
                        "description": "Cheat day",
                        "example": false
                    },
                    "finishedAt": {
                        "type": "string",
                        "description": "Finished at",
                        "example": "2020-04-20T15:00:00.000Z"
                    },
                    "completed": {
                        "type": "boolean",
                        "description": "Completed",
                        "example": false
                    },
                    "tasks": {
                        "type": "array",
                        "items": {
                            "type": "object",
                            "properties": {
                                "title": {
                                    "type": "string",
                                    "description": "Name",
                                    "example": "Push ups"
                                },
                                "score": {
                                    "type": "number",
                                    "description": "Score",
                                    "example": 10
                                },
                                "completed": {
                                    "type": "boolean",
                                    "description": "Completed",
                                    "example": false
                                }
                            }
                        }
                    }
                }
            },
            "RoutineSchema": {
                "type": "object",
                "properties": {
                    "message": {
                        "type": "string",
                        "description": "Message",
                        "example": "Success"
                    },
                    "routine": {
                        "type": "object",
                        "properties": {
                            "deadline": {
                                "type": "string",
                                "description": "Deadline",
                                "example": "2020-04-20T15:00:00.000Z"
                            },
                            "completed": {
                                "type": "boolean",
                                "description": "Completed",
                                "example": false
                            },
                            "cheatDay": {
                                "type": "boolean",
                                "description": "Cheat day",
                                "example": false
                            },
                            "finishedAt": {
                                "type": "string",
                                "description": "Finished at",
                                "example": "2020-04-20T15:00:00.000Z"
                            },
                            "tasks": {
                                "type": "array",
                                "items": {
                                    "type": "object",
                                    "properties": {
                                        "title": {
                                            "type": "string",
                                            "description": "Name",
                                            "example": "Push ups"
                                        },
                                        "score": {
                                            "type": "number",
                                            "description": "Score",
                                            "example": 10
                                        },
                                        "completed": {
                                            "type": "boolean",
                                            "description": "Completed",
                                            "example": false
                                        },
                                        "user": {
                                            "type": "string",
                                            "description": "User ID",
                                            "example": "5e9f9b9b9b9b9b9b9b9b9b9b"
                                        },
                                        "createdAt": {
                                            "type": "string",
                                            "description": "Creation date",
                                            "example": "2020-04-20T15:00:00.000Z"
                                        },
                                        "updatedAt": {
                                            "type": "string",
                                            "description": "Update date",
                                            "example": "2020-04-20T15:00:00.000Z"
                                        }
                                    }
                                }
                            }
                        }
                    }
                }
            },
            "DraftCheckTaskSchema": {
                "type": "object",
                "properties": {
                    "completed": {
                        "type": "boolean",
                        "description": "Completion status",
                        "example": false
                    },
                    "taskId": {
                        "type": "number",
                        "description": "Task Identifier",
                        "example": "987499374857294"
                    }
                }
            },
            "CreateRoutineRequest": {
                "type": "object",
                "properties": {
                    "deadline": {
                        "type": "string",
                        "description": "Deadline for the routine",
                        "example": "2020-04-20T15:00:00.000Z"
                    },
                    "completed": {
                        "type": "boolean",
                        "description": "Completion status of the routine",
                        "example": false
                    },
                    "cheatDay": {
                        "type": "boolean",
                        "description": "Indicates if it is a cheat day",
                        "example": false
                    },
                    "tasks": {
                        "type": "array",
                        "items": {
                            "type": "object",
                            "properties": {
                                "title": {
                                    "type": "string",
                                    "description": "Title of the task",
                                    "example": "Push ups"
                                },
                                "score": {
                                    "type": "number",
                                    "description": "Score associated with the task",
                                    "example": 10
                                },
                                "completed": {
                                    "type": "boolean",
                                    "description": "Completion status of the task",
                                    "example": false
                                }
                            },
                            "required": ["title", "score", "completed"]
                        }
                    }
                },
                "required": ["deadline", "completed", "cheatDay", "tasks"]
            },
            "AddTasksRequest": {
                "type": "object",
                "properties": {
                    "tasksToAdd": {
                        "type": "array",
                        "items": {
                            "type": "object",
                            "properties": {
                                "title": {
                                    "type": "string",
                                    "description": "Title of the task",
