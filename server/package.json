{
  "name": "shad-ws",
  "version": "1.0.0",
  "description": "Shad app webserver",
  "scripts": {
    "eslint": "eslint src/server.ts",
    "postinstall": "chmod +x ./scripts/dev.sh",
    "setup-actions": "chmod +x ./scripts/setup-github-actions.sh; ./scripts/setup-github-actions.sh",
    "build": "tsc",
    "lint": "./node_modules/eslint/bin/eslint.js  src --ext .ts",
    "dev": "./scripts/dev.sh",
    "dev-server": "NODE_ENV=development nodemon",
    "ts-node-server": "ts-node src/server.ts",
    "start": "node dist/src/server.js",
    "test": "jest",
    "fix-lint": "./node_modules/eslint/bin/eslint.js  src --ext .ts --fix"
  },
  "dependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/jsonwebtoken": "^9.0.1",
    "bcrypt": "^5.1.0",
    "body-parser": "1.20.1",
    "compression": "1.7.4",
    "dotenv": "16.0.3",
    "express": "4.18.2",
    "joi": "17.7.0",
    "jsonwebtoken": "^9.0.0",
    "mongoose": "6.8.3",
    "node-cron": "^3.0.2",
    "swagger-ui-express": "4.6.0",
    "winston": "3.8.2",
    "winston-transport": "4.5.0"
  },
  "devDependencies": {
    "@types/compression": "1.7.2",
    "@types/express": "4.17.15",
    "@types/hapi__joi": "17.1.9",
    "@types/jest": "29.2.5",
    "@types/node": "18.11.18",
    "@types/node-cron": "^3.0.7",
    "@types/supertest": "2.0.12",
    "@types/swagger-ui-express": "4.1.3",
    "jest": "29.3.1",
    "jest-express": "1.12.0",
    "nodemon": "2.0.20",
    "supertest": "6.3.3",
    "ts-jest": "29.0.5",
    "ts-node": "10.9.1",
    "typescript": "4.9.4"
  },
